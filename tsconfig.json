{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",

        "declaration": false,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "newLine": "LF",
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules"
    ],
    "compileOnSave": false
}
