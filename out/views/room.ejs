<%- include('./_partial/header') %>
  <div class="content">

    <h1>Room <%= roomNumber %></h1>

    <table class="stripe">
      <thead>
        <tr>
            <th>Date</th>
            <th>Status</th>
        </tr>
      </thead>

      <tbody>
        <% incidents.forEach(function (value, key) { %>
          <tr>
            <td><%= value.Updated %></td>
            <td><%= value.Type %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>

  </div>
